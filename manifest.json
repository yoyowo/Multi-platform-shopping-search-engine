{
  "manifest_version": 3,
  "name": "多平台購物搜尋器",
  "version": "1.0",
  "description": "一次在多個購物網站中開啟搜尋結果分頁。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  }
}